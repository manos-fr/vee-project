  "scripts": {
    "start": "npx nx run vee:serve",
    "backend": "npx nx run vee-be:serve",
    "build": "nx build",
    "lint": "nx lint",
    "infra": "docker compose -p vee -f infrastructure/docker-compose.yaml up",
    "hasura": "cd infrastructure/hasura && hasura migrate apply --database-name=default && hasura metadata apply && hasura metadata reload && hasura console --console-port=9697 --admin-secret=jijikos",
    "reset-infra": "docker compose -p vee -f infrastructure/docker-compose.yaml down -v",
    "docker-clear": "docker rmi $(docker image ls -q --filter reference=vee-vee-be)",
    "update-schema": "cd infrastructure/hasura && hasura migrate apply && hasura metadata apply",
    "seed": "cd infrastructure/hasura && hasura seed apply",
    "codegen": "cd apps/vee && graphql-codegen --watch"
  },
